    <string name="pastep1">Step 1).\tGo to Settings and Click Advanced settings</string>
    <string name="pastep2">Step 2).\tClick Battery manager</string>
    <string name="pastep3">Step 3).\tGo to Protected apps</string>
    <string name="pastep4">Step 4).\tFind and enable UoM Wireless application</string>

    <string name="step1">Step 1).\tGo to Settings and Click Apps</string>
    <string name="step2">Step 2).\tClick Settings icon</string>
    <string name="step3">Step 3).\tGo to Special access</string>
    <string name="step4">Step 4).\tSelect Ignore Optimizations</string>
    <string name="step5">Step 5).\tList All apps</string>
    <string name="step6">Step 6).\tClick on UoM Wireless application</string>
    <string name="step7">Step 7).\tCheck Allow option and hit OK</string>

